{
  "name": "api-pg-hba",
  "version": "0.1.0",
  "description": "api-pg-hba",
  "main": "dist/src/index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run build-ts && npm run lint",
    "serve": "node dist/src/index.js",
    "watch-node": "nodemon dist/src/index.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --detectOpenHandles --forceExit --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "eslint \"**/*.{js,ts}\" --quiet --fix",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/src/index.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "inversify": "^5.0.1",
    "inversify-express-utils": "^6.3.2",
    "moment": "^2.26.0",
    "pg": "^8.3.0",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.3.3"
  },
  "devDependencies": {
    "@types/concurrently": "^5.2.0",
    "@types/dotenv": "^8.2.0",
    "@types/eslint": "^6.8.1",
    "@types/express": "^4.17.6",
    "@types/handlebars": "^4.1.0",
    "@types/jest": "^25.2.3",
    "@types/moment": "^2.13.0",
    "@types/mongoose": "^5.7.21",
    "@types/node": "^14.0.5",
    "@types/pg": "^7.14.4",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "concurrently": "^5.2.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^26.0.0",
    "typescript": "^3.9.7"
  }
}
